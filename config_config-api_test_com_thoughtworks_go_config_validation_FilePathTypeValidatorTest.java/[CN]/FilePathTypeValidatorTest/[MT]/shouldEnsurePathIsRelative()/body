{
  assertThat(filePathTypeValidator.isPathValid(".."),is(false));
  assertThat(filePathTypeValidator.isPathValid("../a"),is(false));
  assertThat(filePathTypeValidator.isPathValid(" "),is(false));
  assertThat(filePathTypeValidator.isPathValid("./a"),is(false));
  assertThat(filePathTypeValidator.isPathValid(". "),is(false));
  assertThat(filePathTypeValidator.isPathValid(" ."),is(false));
  assertThat(filePathTypeValidator.isPathValid("abc"),is(true));
}
