{
  String askIfPassinghasAFailure="PREFIX xunit: <" + XUnitOntology.URI + "> "+ "PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> "+ "ASK WHERE { "+ "?testCase a xunit:TestCase ; "+ "xunit:testCaseName \"oneThatWorks\"^^xsd:string ; "+ "xunit:hasFailure ?failure "+ "}";
  assertAskIsFalse(minimalDataGraph,askIfPassinghasAFailure);
}
