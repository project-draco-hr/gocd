{
  LogDirectory logDirectory=new LogDirectory(existingLogFolder.getAbsolutePath());
  logDirectory.update(log4jPropertiesFile,new Validation());
  assertThat(readFileToString(log4jPropertiesFile),containsString("log4j.appender.FileAppender.File=" + existingLogFolder.getAbsolutePath() + "/go-server.log"));
}
