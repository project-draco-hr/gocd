{
  String pipelineXmlPartial="<pipeline name=\"pipeline\" isLocked=\"false\">\n" + "  <materials>\n" + "    <hg url=\"/hgrepo\"/>\n"+ "  </materials>\n"+ "  <stage name=\"mingle\">\n"+ "    <jobs>\n"+ "      <job name=\"functional\">\n"+ "        <artifacts>\n"+ "          <log src=\"artifact1.xml\" dest=\"cruise-output\" />\n"+ "        </artifacts>\n"+ "      </job>\n"+ "    </jobs>\n"+ "  </stage>\n"+ "</pipeline>\n";
  PipelineConfig pipeline=xmlLoader.fromXmlPartial(toInputStream(pipelineXmlPartial),PipelineConfig.class);
  assertThat(pipeline.hasExplicitLock(),is(true));
  assertThat(pipeline.explicitLock(),is(false));
}
