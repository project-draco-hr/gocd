{
  String message=String.format("[%s] %s %s on %s [%s] at %s",GoConstants.PRODUCT_NAME,action,jobIdentifier.buildLocatorForDisplay(),agentIdentifier.getHostName(),currentWorkingDirectory,timeProvider.currentTime());
  if (LOG.isDebugEnabled()) {
    LOG.debug(message);
  }
  consumeLine(message);
}
