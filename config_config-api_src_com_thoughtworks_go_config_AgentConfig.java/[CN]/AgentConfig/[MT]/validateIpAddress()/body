{
  String address=getIpAddress();
  if (address == null) {
    return;
  }
  if (address.trim().isEmpty()) {
    addError("ipAddress","IpAddress cannot be empty if it is present.");
    return;
  }
  try {
    IpAddress.create(address);
  }
 catch (  Exception e) {
    addError("ipAddress","IpAddress is invalid.");
  }
}
