{
  this.ruby=runtime;
  this.attrClass=(RubyClass)runtime.getClassFromPath("Nokogiri::XML::SAX::Parser::Attribute");
  this.object=object;
  lines=new ArrayDeque<Integer>();
  columns=new ArrayDeque<Integer>();
  String objectName=object.getMetaClass().getName();
