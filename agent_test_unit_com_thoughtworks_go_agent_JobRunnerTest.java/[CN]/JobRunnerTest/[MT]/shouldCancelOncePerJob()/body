{
  runner.setWork(work);
  runner.handleInstruction(new AgentInstruction(true),new AgentRuntimeInfo(agentIdentifier,AgentRuntimeStatus.Idle,currentWorkingDirectory(),"cookie",null));
  assertThat(work.getCallCount(),is(1));
  runner.handleInstruction(new AgentInstruction(true),new AgentRuntimeInfo(agentIdentifier,AgentRuntimeStatus.Idle,currentWorkingDirectory(),"cookie",null));
  assertThat(work.getCallCount(),is(1));
}
