{
  runner.setWork(work);
  runner.handleInstruction(new AgentInstruction(true),AgentRuntimeInfo.fromAgent(agentIdentifier,"cookie",null));
  assertThat(work.getCallCount(),is(1));
  runner.handleInstruction(new AgentInstruction(true),AgentRuntimeInfo.fromAgent(agentIdentifier,"cookie",null));
  assertThat(work.getCallCount(),is(1));
}
