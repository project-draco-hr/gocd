{
  if (typeName.equals(CRExecTask_1.TYPE_NAME))   return CRExecTask_1.class;
  if (typeName.equals(CRBuildTask_1.RAKE_TYPE_NAME))   return CRBuildTask_1.class;
  if (typeName.equals(CRBuildTask_1.ANT_TYPE_NAME))   return CRBuildTask_1.class;
  if (typeName.equals(CRBuildTask_1.NANT_TYPE_NAME))   return CRNantTask_1.class;
  if (typeName.equals(CRPluggableTask_1.TYPE_NAME))   return CRPluggableTask_1.class;
 else   throw new JsonParseException(String.format("Invalid or unknown material type '%s'",typeName));
}
