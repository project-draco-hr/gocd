{
  ArrayList<List<String>> splittedStrings=new ArrayList<List<String>>();
  if (list.isEmpty()) {
    return splittedStrings;
  }
  splittedStrings.add(new ArrayList<String>());
  for (  String line : list) {
    if (line.equals(splitOn)) {
      splittedStrings.add(new ArrayList<String>());
    }
 else {
      last(splittedStrings).add(line);
    }
  }
  return splittedStrings;
}
