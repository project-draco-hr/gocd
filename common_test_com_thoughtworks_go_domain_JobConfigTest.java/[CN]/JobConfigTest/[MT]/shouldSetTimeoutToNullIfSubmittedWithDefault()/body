{
  config.setConfigAttributes(m(JobConfig.NAME,"foo-job","timeoutType",JobConfig.DEFAULT_TIMEOUT,JobConfig.TIMEOUT,"",JobConfig.TASKS,m(Tasks.TASK_OPTIONS,"exec","exec",m(Task.TASK_TYPE,"exec",ExecTask.COMMAND,"ls",ExecTask.ARGS,"-la",ExecTask.WORKING_DIR,"/tmp"))));
  assertThat(config.getTimeout(),is(nullValue()));
}
