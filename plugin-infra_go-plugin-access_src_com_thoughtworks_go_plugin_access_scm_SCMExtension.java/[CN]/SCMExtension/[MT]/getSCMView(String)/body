{
  return pluginRequestHelper.submitRequest(pluginId,REQUEST_SCM_VIEW,new PluginInteractionCallback<SCMView>(){
    @Override public String requestBody(    String resolvedExtensionVersion){
      return null;
    }
    @Override public Map<String,String> requestParams(    String resolvedExtensionVersion){
      return null;
    }
    @Override public SCMView onSuccess(    String responseBody,    String resolvedExtensionVersion){
      return messageHandlerMap.get(resolvedExtensionVersion).responseMessageForSCMView(responseBody);
    }
  }
);
}
