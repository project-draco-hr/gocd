{
  JsonMap json=new JsonMap();
  json.put("first","1");
  json.put("second",jsonList("a","b","c"));
  JsonTester tester=new JsonTester(json);
  tester.shouldContain("{ 'first' : '1' }");
  tester.shouldContain("{ 'second' : [ " + "    'a',       " + "    'c',       "+ "    'b'        "+ "  ]            "+ "}              ");
}
