{
  RubyClass encodingClass=runtime.getEncoding();
  if (encodingClass == null) {
    utf8=ascii8bit=null;
    encodings=null;
  }
 else {
    ThreadContext context=runtime.getCurrentContext();
    utf8=new WeakReference<RubyEncoding>((RubyEncoding)RubyEncoding.find(context,encodingClass,runtime.newString("utf-8")));
    ascii8bit=new WeakReference<RubyEncoding>((RubyEncoding)RubyEncoding.find(context,encodingClass,runtime.newString("ascii-8bit")));
    encodings=new HashMap<String,WeakReference<RubyEncoding>>();
  }
}
