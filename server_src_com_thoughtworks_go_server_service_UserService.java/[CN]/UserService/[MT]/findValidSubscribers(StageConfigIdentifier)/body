{
  Users users=userDao.findNotificationSubscribingUsers();
  return users.filter(new Filter<User>(){
    public boolean matches(    User user){
      return user.hasSubscribedFor(identifier.getPipelineName(),identifier.getStageName()) && securityService.hasViewPermissionForPipeline(user.getName(),identifier.getPipelineName());
    }
  }
);
}
