{
  assertThat(Pattern.matches("a\\.doc","a.doc"),is(true));
  assertThat(Pattern.matches("\\p{Punct}","*"),is(true));
  assertThat(Pattern.matches("\\p{Punct}","{"),is(true));
  assertThat(Pattern.matches("\\p{Punct}","]"),is(true));
  assertThat(Pattern.matches("\\p{Punct}","-"),is(true));
  assertThat(Pattern.matches("\\p{Punct}","."),is(true));
}
