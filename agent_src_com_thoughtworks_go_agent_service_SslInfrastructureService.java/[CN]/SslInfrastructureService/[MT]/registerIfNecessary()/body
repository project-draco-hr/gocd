{
  registered=keyStoreManager.hasCertificates(CHAIN_ALIAS,AGENT_CERTIFICATE_FILE,AGENT_STORE_PASSWORD) && GuidService.guidPresent();
  if (!registered) {
    LOGGER.info("[Agent Registration] Starting to register agent");
    register();
    createSslInfrastructure();
    registered=true;
    LOGGER.info("[Agent Registration] Successfully registered agent");
  }
}
