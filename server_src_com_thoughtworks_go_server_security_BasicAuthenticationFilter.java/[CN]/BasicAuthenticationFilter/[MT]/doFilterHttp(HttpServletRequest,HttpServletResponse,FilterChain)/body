{
  try {
    isProcessingBasicAuth.set(true);
    super.doFilterHttp(httpRequest,httpResponse,chain);
  }
 catch (  Exception e) {
    LOG.error(e.getMessage());
    LOG.debug(e.getMessage(),e);
    handleException(httpRequest,httpResponse,e);
  }
 finally {
    isProcessingBasicAuth.set(false);
  }
}
