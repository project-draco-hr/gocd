{
  Date expectedLatest=date(2008,10,12);
  Date actualLatest=new JobInstances(completed(completed("job1"),JobResult.Failed,expectedLatest),completed(completed("job1"),JobResult.Failed,date(2008,10,11)),completed(completed("job1"),JobResult.Failed,date(2008,10,5))).latestTransitionDate();
  assertThat(actualLatest,is(expectedLatest));
}
