{
  SCMs scms=getSCMs();
  String id=crPluggableScmMaterial.getScmId();
  SCM scmConfig=scms.find(id);
  if (scmConfig == null)   throw new ConfigConvertionException(String.format("Failed to find referenced scm '%s'",id));
  return new PluggableSCMMaterialConfig(toMaterialName(crPluggableScmMaterial.getName()),scmConfig,crPluggableScmMaterial.getDirectory(),toFilter(crPluggableScmMaterial.getFilterIgnore()));
}
