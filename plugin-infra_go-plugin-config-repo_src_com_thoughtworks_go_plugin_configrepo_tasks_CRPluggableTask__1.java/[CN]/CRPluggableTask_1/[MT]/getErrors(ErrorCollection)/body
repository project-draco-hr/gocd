{
  if (this.pluginConfiguration != null)   this.pluginConfiguration.getErrors(errors);
 else   errors.add(this,"Pluggable task has no plugin configuration");
  if (this.configuration != null) {
    for (    CRConfigurationProperty_1 p : configuration) {
      p.getErrors(errors);
    }
  }
  validateType(errors);
  validateKeyUniqueness(errors);
  validateOnCancel(errors);
}
