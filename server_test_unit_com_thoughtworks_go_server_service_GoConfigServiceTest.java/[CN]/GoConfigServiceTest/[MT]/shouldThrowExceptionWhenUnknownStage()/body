{
  expectLoadForEditing(configWith(createPipelineConfig("pipeline","stage","build")));
  try {
    goConfigService.buildSaver("pipeline","unknown",1).asXml();
    fail("Unknown stage");
  }
 catch (  Exception e) {
    assertThat(e.getMessage(),is("Stage 'unknown' not found in pipeline 'pipeline'"));
  }
}
