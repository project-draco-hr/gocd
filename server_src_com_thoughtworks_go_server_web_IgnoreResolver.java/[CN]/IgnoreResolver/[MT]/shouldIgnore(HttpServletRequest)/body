{
  try {
    return isGoingToAbout(request) || isGoingToPlugin(request) || isDownloadAgentJar(request)|| isGoingToLogin(request)|| isGoingToServerInfo(request)|| isPost(request)|| isPut(request)|| isStaticFile(request);
  }
 catch (  URISyntaxException e) {
    throw new RuntimeException(e);
  }
}
