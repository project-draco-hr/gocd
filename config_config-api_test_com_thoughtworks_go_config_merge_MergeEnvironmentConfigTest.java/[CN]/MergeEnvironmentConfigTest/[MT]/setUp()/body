{
  singleEnvironmentConfig=new MergeEnvironmentConfig(new BasicEnvironmentConfig(new CaseInsensitiveString("UAT")));
  pairEnvironmentConfig=new MergeEnvironmentConfig(new BasicEnvironmentConfig(new CaseInsensitiveString("UAT")),new BasicEnvironmentConfig(new CaseInsensitiveString("UAT")));
  super.environmentConfig=pairEnvironmentConfig;
}
