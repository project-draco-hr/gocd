{
  ByteArrayOutputStream bytes=new ByteArrayOutputStream();
  try {
    GZIPOutputStream zip=new GZIPOutputStream(bytes);
    ObjectOutputStream output=new ObjectOutputStream(zip);
    try {
      output.writeObject(msg);
    }
  finally {
      output.close();
    }
  }
 catch (  IOException e) {
    throw bomb(e);
  }
  return bytes.toByteArray();
}
