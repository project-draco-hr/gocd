{
  ArtifactsDiskSpaceFullChecker artifactsDiskSpaceFullChecker=new ArtifactsDiskSpaceFullChecker(systemEnvironment,goConfigService);
  DatabaseDiskSpaceFullChecker databaseDiskSpaceFullChecker=new DatabaseDiskSpaceFullChecker(systemEnvironment,goConfigService);
  return new CompositeChecker(artifactsDiskSpaceFullChecker,databaseDiskSpaceFullChecker);
}
