{
  if (!noDecl) {
    buffer.append("<?xml version=\"");
    buffer.append(document.getXmlVersion());
    buffer.append("\"");
    if (encoding != null) {
      buffer.append(" encoding=\"");
      buffer.append(encoding);
      buffer.append("\"");
    }
    buffer.append("?>\n");
  }
  return true;
}
