{
  AuthenticationPluginConfiguration configuration=messageHandler.responseMessageForPluginConfiguration("{\"display-name\":\"display-name\",\"display-image-url\":\"display-image-url\",\"supports-password-based-authentication\":true,\"supports-user-search\":true}");
  assertThat(configuration,is(new AuthenticationPluginConfiguration("display-name","display-image-url",true,true)));
}
