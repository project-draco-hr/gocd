  context.checking(new Expectations(){
{
      allowing(artifactsDirHolder).getArtifactsDir();
      will(returnValue(folder));
    }
  }
);
  zipArtifactCache=new ZipArtifactCache(this.artifactsDirHolder,new ZipUtil());
