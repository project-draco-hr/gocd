  this.branch=branch;
  this.flyweightName=flyweightName;
  this.workspace=workspace;
  this.projectPath=projectPath;
  this.domain=domain;
  this.configuration=configuration;
  this.fingerprint=toOldMaterial(null,null,null).getFingerprint();
}
