{
  Revision revision=revisionContext.getLatestRevision();
  String workingDir=material.workingdir(new File(baseDir)).getPath();
  UrlArgument url=material.getUrlArgument();
  return compose(echoWithPrefix("Start updating %s at revision %s from %s",material.updatingTarget(),revision.getRevision(),url.forDisplay()),secret(url.hostInfoForCommandline(),url.hostInfoForDisplay()),cloneIfNeeded(workingDir,revisionContext.numberOfModifications() + 1),fetchRemote(workingDir),unshallowIfNeeded(workingDir,revision,new Integer[]{GitMaterial.UNSHALLOW_TRYOUT_STEP,Integer.MAX_VALUE}),resetWorkingCopy(workingDir,revision),echoWithPrefix("Done.\n"));
}
