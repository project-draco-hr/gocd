{
  ByteList bl=value.getByteList();
  int len=bl.length();
  for (int pos=0; pos < len - 1; pos++) {
    int b=bl.get(pos);
    if (Character.isWhitespace(b))     continue;
switch (b) {
case '[':
      return matchClosingBrace(bl,pos,len,']');
case '{':
    return matchClosingBrace(bl,pos,len,'}');
default :
  return false;
}
}
return false;
}
