{
  ByteList message=new ByteList(ByteList.plain("partial character in source, " + "but hit end near "));
  int start=surrogatePairStart != -1 ? surrogatePairStart : charStart;
  message.append(src,start,srcEnd - start);
  return Utils.newException(context,Utils.M_PARSER_ERROR,context.getRuntime().newString(message));
}
