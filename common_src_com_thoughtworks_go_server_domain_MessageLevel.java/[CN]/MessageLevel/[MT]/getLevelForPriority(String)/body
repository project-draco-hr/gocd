{
  if ("debug".equalsIgnoreCase(displayName)) {
    return DEBUG;
  }
 else   if ("info".equalsIgnoreCase(displayName)) {
    return INFO;
  }
 else   if ("warn".equalsIgnoreCase(displayName)) {
    return WARN;
  }
 else   if ("error".equalsIgnoreCase(displayName)) {
    return ERROR;
  }
 else {
    return UNKNOWN;
  }
}
