{
  new DefaultPluginManager(monitor,registry,goPluginOSGiFramework,jarChangeListener,null,pluginWriter,pluginValidator,systemEnvironment,pluginsZipListener).startInfrastructure();
  InOrder inOrder=inOrder(goPluginOSGiFramework,monitor);
  inOrder.verify(goPluginOSGiFramework).start();
  inOrder.verify(monitor).start();
}
