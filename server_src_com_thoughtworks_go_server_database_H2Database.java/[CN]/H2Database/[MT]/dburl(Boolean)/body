{
  return "jdbc:h2:" + env.getDbPath() + "/"+ env.get(SystemEnvironment.GO_DATABASE_NAME)+ ";DB_CLOSE_DELAY=-1"+ ";DB_CLOSE_ON_EXIT=FALSE"+ ";MVCC="+ mvccEnabled.toString().toUpperCase()+ ";CACHE_SIZE="+ env.getCruiseDbCacheSize()+ ";TRACE_LEVEL_FILE="+ env.getCruiseDbTraceLevel()+ ";TRACE_MAX_FILE_SIZE="+ env.getCruiseDbTraceFileSize()+ ";DATABASE_EVENT_LISTENER='"+ H2EventListener.class.getName()+ "'";
}
