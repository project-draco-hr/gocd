{
  Iterable<RevCommit> revisions=revisions();
  Iterator<RevCommit> iterator=revisions.iterator();
  RevCommit current, next=null;
  while (iterator.hasNext()) {
    current=iterator.next();
    if (current.equals(givenCommit)) {
      if (iterator.hasNext()) {
        next=iterator.next();
        break;
      }
    }
  }
  return next;
}
