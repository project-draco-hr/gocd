{
  extension=mock(ConfigRepoExtension.class);
  service=new GoConfigPluginService(extension,mock(ConfigCache.class),ConfigElementImplementationRegistryMother.withNoPlugins(),mock(MetricsProbeService.class),mock(CachedFileGoConfig.class));
  partialConfig=new CRPartialConfig();
  parseResult=new CRParseResult(partialConfig);
  when(extension.parseDirectory(any(String.class),any(String.class),any(Collection.class))).thenReturn(parseResult);
}
