{
  JsonTester jsonTester=new JsonTester(presenter.toJson());
  jsonTester.shouldContain("{    'groups' : [ {" + "     'config' : {" + "         'stages' : [ "+ "             {'name' : 'dev', isAutoApproved : 'true'},"+ "             {'name' : 'ft', isAutoApproved : 'false'}"+ "          ]"+ "      }"+ "    } ] "+ "}");
}
