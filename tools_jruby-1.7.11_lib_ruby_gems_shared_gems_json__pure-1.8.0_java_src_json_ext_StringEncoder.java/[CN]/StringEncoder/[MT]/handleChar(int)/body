{
switch (c) {
case '"':
case '\\':
    escapeChar((char)c);
  break;
case '\n':
escapeChar('n');
break;
case '\r':
escapeChar('r');
break;
case '\t':
escapeChar('t');
break;
case '\f':
escapeChar('f');
break;
case '\b':
escapeChar('b');
break;
default :
if (c >= 0x20 && c <= 0x7f || (c >= 0x80 && !asciiOnly)) {
quoteStart();
}
 else {
quoteStop(charStart);
escapeUtf8Char(c);
}
}
}
