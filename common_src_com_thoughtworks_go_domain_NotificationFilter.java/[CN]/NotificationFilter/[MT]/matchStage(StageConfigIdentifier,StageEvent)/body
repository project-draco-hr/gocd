{
  boolean anyPipelineAndStage=this.pipelineName.equals(GoConstants.ANY_PIPELINE) && this.stageName.equals(GoConstants.ANY_STAGE);
  if (anyPipelineAndStage && this.event.include(event)) {
    return true;
  }
  return stageIdentifier.getPipelineName().equals(pipelineName) && (stageIdentifier.getStageName().equals(stageName) || stageName.equals(GoConstants.ANY_STAGE)) && this.event.include(event);
}
